{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "defexception",
    "defprotocol",
    "defstruct",
    "def.+(.+\\\\.+).+do",
    "^\\s+use\\s+"
  ],

  "custom_stop_words": [
  ],

  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/",
    "minimum_coverage": 0
  },
  "terminal_options": {
    "file_column_width": 40
  },

  "skip_files": [
      "test",
      "lib/shoe_shop_web.ex",
      "lib/shoe_shop_web/application.ex",
      "lib/shoe_shop_web/router.ex",
      "lib/shoe_shop_web/views/error_helpers.ex",
      "lib/shoe_shop_web/telemetry.ex"


    ]  
  
}